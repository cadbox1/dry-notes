{
  "name": "very-nested",
  "private": true,
  "packageManager": "yarn@3.2.3",
  "workspaces": {
    "packages": [
      "packages/*"
    ]
  },
  "scripts": {
    "prepare": "lerna run prepare",
    "clean": "lerna run clean",
    "bump": "lerna version --force-publish",
    "deploy": "npm run bump && cd packages/viewer && npm publish"
  },
  "devDependencies": {
    "lerna": "^3.20.2"
  },
  "resolutions": {
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-redux": "^8.0.2",
    "@types/react": "17.0.2",
    "@types/react-dom": "17.0.2"
  }
}
